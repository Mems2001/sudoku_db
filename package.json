    "url": "https://github.com/Mems2001/sudoku_db/issues"
  },
  "homepage": "https://github.com/Mems2001/sudoku_db#readme",
  "dependencies": {
    "bcrypt": "^5.1.1",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "helmet": "^8.0.0",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.14.0",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.37.6",
    "sequelize-cli": "^6.6.2",
    "uuid": "^11.1.0"
